.custom-tab-bar-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.custom-tab-bar-wrapper .text {
  font-size: 20rpx;
}

/* 自定义图标样式 */
.custom-icon {
  font-size: 48rpx;
  line-height: 1;
  margin-bottom: 4rpx;
}

.bookmark-icon {
  color: #666;
  transition: color 0.3s ease;
}

/* 激活状态下的图标颜色 */
.t-tab-bar-item.t-is-active .bookmark-icon {
  color: #0052d9;
}
